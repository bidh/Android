<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >
    <TextView
        android:id="@+id/bikeUUID"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:text="TextView" />

    <TextView
        android:id="@+id/bike_name_list_ride"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/bikeUUID"
        android:text="TextView" />

    <TextView
        android:id="@+id/bike_start_list_ride"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/bike_name_list_ride"
        android:text="TextView" />

    <TextView
        android:id="@+id/bike_start_date_list_ride"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/bike_start_list_ride"
        android:text="TextView" />

    <TextView
        android:id="@+id/bike_end_list_ride"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/bike_start_date_list_ride"
        android:text="TextView" />

    <TextView
        android:id="@+id/bike_end_date_list_ride"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/bike_end_list_ride"
        android:text="TextView" />

    <TextView
        android:id="@+id/bike_price_list_ride"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/bike_end_date_list_ride"
        android:text="TextView" />
</RelativeLayout>